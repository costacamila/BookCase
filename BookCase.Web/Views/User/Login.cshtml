@{ ViewData["Title"] = "Login";}@model BookCase.Web.ViewModel.LoginViewModel<div class="text-center">    <h1 class="display-4">BookCase</h1></div><div asp-validation-summary="All" class="text-danger"></div><form asp-action="Login" asp-controller="User" method="post" asp-route-returnurl="@ViewBag.ReturnUrl">    <div class="form-group">        <label asp-for="Email">Email</label>        <input type="email" asp-for="Email" class="form-control" placeholder="Email" required>    </div>    <div class="form-group">        <label asp-for="Password">Password</label>        <input type="password" asp-for="Password" class="form-control" placeholder="Password" required>    </div>    <div style="display: flex;">        <div style="margin: 5px;">            <button type="submit" class="btn-sm btn-dark">Entrar</button>        </div>        <div style="margin: 5px;">            <a asp-action="Create" class="btn-sm btn-outline-dark">Criar conta</a>        </div>    </div></form><hr /><a asp-action="Create" class="btn-sm btn-outline-dark">Criar conta</a>